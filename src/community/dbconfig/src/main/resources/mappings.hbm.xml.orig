    <property column="UPDATE_SEQUENCE" name="updateSequence"/>
    <many-to-one cascade="all" class="org.geoserver.catalog.MetadataMap" column="METADATA" foreign-key="fk_geoserver_metadata" lazy="false" name="metadata"/>
